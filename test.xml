<?xml version="1.0" encoding="UTF-8"?>


<mdb:MD_Metadata xmlns="http://standards.iso.org/iso/19115/-3/mdb/1.0"
  xmlns:mdb="http://standards.iso.org/iso/19115/-3/mdb/1.0"
  xmlns:cit="http://standards.iso.org/iso/19115/-3/cit/1.0"
  xmlns:dqc="http://standards.iso.org/iso/19157/-2/dqc/1.0"
  xmlns:gco="http://standards.iso.org/iso/19115/-3/gco/1.0"
  xmlns:gex="http://standards.iso.org/iso/19115/-3/gex/1.0"
  xmlns:lan="http://standards.iso.org/iso/19115/-3/lan/1.0"
  xmlns:mcc="http://standards.iso.org/iso/19115/-3/mcc/1.0"
  xmlns:mco="http://standards.iso.org/iso/19115/-3/mco/1.0"
  xmlns:mrd="http://standards.iso.org/iso/19115/-3/mrd/1.0"
  xmlns:mri="http://standards.iso.org/iso/19115/-3/mri/1.0"
  xmlns:mrl="http://standards.iso.org/iso/19115/-3/mrl/1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://standards.iso.org/iso/19115/-3/mdb/1.0/mdb.xsd">
  <mdb:contact>
    <cit:CI_Responsibility>
      <cit:role>
        <cit:CI_RoleCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#CI_RoleCode" codeListValue=""></cit:CI_RoleCode>
      </cit:role>
      <cit:party>
        <cit:CI_Organisation>
          <!-- name or logo is mandatory by Schematron-enforced constraint -->
          <cit:name>
            <gco:CharacterString></gco:CharacterString>
          </cit:name>
          <cit:contactInfo>
            <cit:CI_Contact>
              <cit:contactInstructions>
                <gco:CharacterString></gco:CharacterString>
              </cit:contactInstructions>
              <cit:phone>
                <cit:CI_Telephone>
                  <gco:CharacterString></gco:CharacterString>
                </cit:CI_Telephone>
              </cit:phone>
              <cit:address>
                <cit:CI_Address>
                  <cit:city>
                    <gco:CharacterString></gco:CharacterString>
                  </cit:city>
                  <cit:country>
                    <gco:CharacterString></gco:CharacterString>
                  </cit:country>
                  <cit:electronicMailAddress>
                    <gco:CharacterString></gco:CharacterString>
                  </cit:electronicMailAddress>
                </cit:CI_Address>
              </cit:address>
            </cit:CI_Contact>
          </cit:contactInfo>
        </cit:CI_Organisation>
      </cit:party>
    </cit:CI_Responsibility>
  </mdb:contact>
  <mdb:dateInfo>
    <cit:CI_Date>
      <cit:date>

          <gco:Date>date_created</gco:Date>

      </cit:date>
      <cit:dateType>
        <cit:CI_DateTypeCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/cat/codeLists.xml#creation" codeListValue="Creation"/>
      </cit:dateType>
    </cit:CI_Date>
  </mdb:dateInfo>
  <mdb:metadataStandard>
    <!-- might not be needed -->
    <cit:CI_Citation>
      <cit:title>
        <gco:CharacterString>citation_title</gco:CharacterString>
      </cit:title>
      <cit:edition>
        <gco:CharacterString>citation_edition</gco:CharacterString>
      </cit:edition>
    </cit:CI_Citation>
  </mdb:metadataStandard>

  <mdb:defaultLocale>
    <lan:PT_Locale>
      <lan:language>
        <!-- May not be correct -->
        <gco:CharacterString>fr</gco:CharacterString>
      </lan:language>
      <mdb:country>
        <!-- May not be correct -->
        <gco:CharacterString>CA</gco:CharacterString>
      </mdb:country>
      <lan:characterEncoding>
        <lan:MD_CharacterSetCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/lan/CharacterSetCode.xml" codeListValue="utf8">UTF-8</lan:MD_CharacterSetCode>
      </lan:characterEncoding>
    </lan:PT_Locale>
  </mdb:defaultLocale>
  <mdb:metdataScope>
    <mdb:resourceScope>
      <mcc:MD_MetadataScope>
        <gco:CharacterString>resource_type</gco:CharacterString>
      </mcc:MD_MetadataScope>
    </mdb:resourceScope>
    <!-- TODO this isn't described well <mdb:name> <gco:CharacterString> </gco:CharacterString> </mdb:name> -->
  </mdb:metdataScope>
  <mdb:metadataIdentifier>
    <gco:CharacterString>id</gco:CharacterString>
  </mdb:metadataIdentifier>

  <mdb:identificationInfo>
    <mri:MD_DataIdentification>
      <!-- cdm_data_type doesn't follow the code list so this is a placeholder <mri:spatialRepresentationType> <mri:MD_SpatialRepresentationTypeCode
      codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#MD_SpatialRepresentationTypeCode" codeSpace="ISOTC211/19115" codeListValue="cdm_data_type">cdm_data_type</cit:MD_SpatialRepresentationTypeCode>
      </mri:spatialRepresentationType> -->
      <mri:resourceConstraints>
        <mco:MD_Constraints>
          <mco:useLimitation>
            <gco:CharacterString>
              license
            </gco:CharacterString>
          </mco:useLimitation>
        </mco:MD_Constraints>
      </mri:resourceConstraints>

      <mri:citation>
        <cit:CI_Citation>
          <cit:title>
            <gco:CharacterString>title</gco:CharacterString>
          </cit:title>
          <!-- mandatory (dataType= creation) by constraint enforced by Schematron rule -->
          <cit:date>
            <cit:CI_Date>
              <cit:date>
                <gco:DateTime>date_metadata_modified</gco:DateTime>
              </cit:date>
              <cit:dateType>
                <cit:CI_DateTypeCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/cat/codeLists.xml#CI_DateTypeCode" codeListValue="publication">publication</cit:CI_DateTypeCode>
              </cit:dateType>
            </cit:CI_Date>
          </cit:date>
        </cit:CI_Citation>
      </mri:citation>
      <mri:abstract>
      <gco:CharacterString>
    summary in fr
    </gco:CharacterString>
        <mri:PT_FreeText>
          <mri:textGroup>
            <mri:LocalisedCharacterString locale="#en">
              summary in eng
            </mri:LocalisedCharacterString>
          </mri:textGroup>
        </mri:PT_FreeText>

      </mri:abstract>
      <mri:purpose>
        <gco:CharacterString>purpose</gco:CharacterString>
      </mri:purpose>
      <mri:status>
        <gco:CharacterString>status</gco:CharacterString>
      </mri:status>
      <!-- mandatory since MD_MetadataScope is not specified, and defaults to 'dataset' enforced by Schematron rule -->
      <mri:topicCategory>
        <mri:MD_TopicCategoryCode>oceans</mri:MD_TopicCategoryCode>
      </mri:topicCategory>
      <!-- a bounding box is required because the mdb:MD_MetadataScope is not specified; enforced by Schematron rule-->
      <mri:extent>
        <gex:EX_Extent>
          <gex:description>
            <gco:CharacterString>extend_description</gco:CharacterString>
          </gex:description>
          <gex:geographicElement>
            <gex:EX_GeographicBoundingBox>
              <gex:extentTypeCode>
                <gco:Boolean>extent_type</gco:Boolean>
              </gex:extentTypeCode>
              <gex:westBoundLongitude>
                <gco:Decimal>geospatial_lon_min</gco:Decimal>
              </gex:westBoundLongitude>
              <gex:eastBoundLongitude>
                <gco:Decimal>geospatial_lon_max</gco:Decimal>
              </gex:eastBoundLongitude>
              <gex:southBoundLatitude>
                <gco:Decimal>geospatial_lat_max</gco:Decimal>
              </gex:southBoundLatitude>
              <gex:northBoundLatitude>
                <gco:Decimal>geospatial_lat_min</gco:Decimal>
              </gex:northBoundLatitude>
            </gex:EX_GeographicBoundingBox>
          </gex:geographicElement>

          <gex:temporalElement>
            <gex:EX_TemporalExtent>
              <gex:extent>
                <gex:TimePeriod>
                  <gex:beginPosition>time_coverage_start</gex:beginPosition>
                  <gex:endPosition>time_coverage_end</gex:endPosition>
                  <gex:endPosition>time_coverage_duration</gex:endPosition>

                </gex:TimePeriod>
              </gex:extent>
            </gex:EX_TemporalExtent>
          </gex:temporalElement>

          <!-- not sure about this section -->
          <gex:EX_VerticalExtent>
            <gex:extent>
              <gex:minimumValue>geospatial_vertical_min</gex:minimumValue>
              <gex:maximumValue>geospatial_vertical_max</gex:maximumValue>
              <gex:verticalCRSId></gex:verticalCRSId>
            </gex:extent>
          </gex:EX_VerticalExtent>

        </gex:EX_Extent>
      </mri:extent>
      <!-- mandatory if resource contains textual content (has to be enforced by inspection) -->
      <mri:defaultLocale>
        <lan:PT_Locale>
          <lan:language>
            <lan:LanguageCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/lan/LanguageCode.xml" codeListValue="eng">English</lan:LanguageCode>
          </lan:language>
          <lan:characterEncoding>
            <lan:MD_CharacterSetCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/lan/CharacterSetCode.xml" codeListValue="utf8">UTF-8</lan:MD_CharacterSetCode>
          </lan:characterEncoding>
        </lan:PT_Locale>
      </mri:defaultLocale>
      <mri:descriptiveKeywords>
        <mri:MD_Keywords>

            <mri:keyword>
              <gco:CharacterString>
                </gco:CharacterString>
            </mri:keyword>

          <!-- <cit:type> <cit:MD_KeywordTypeCode codeList="https://data.noaa.gov/resources/iso19139/schema/resources/Codelist/gmxCodelists.xml#cit:MD_KeywordTypeCode" codeListValue="theme">theme</cit:MD_KeywordTypeCode> </cit:type> -->

        </mri:MD_Keywords>
      </mri:descriptiveKeywords>
      <mri:supplementalInformation>
        <gco:CharacterString>comment</gco:CharacterString>
      </mri:supplementalInformation>

      <cit:onlineResource>
        <cit:CI_OnlineResource>
          <cit:linkage>
            <!-- unsure what this URL is for -->
            <cit:URL></cit:URL>
          </cit:linkage>
          <cit:protocol>
            <gco:CharacterString>WWW:LINK</gco:CharacterString>
          </cit:protocol>
          <cit:processingLevel>
            <gco:CharacterString>processing_level</gco:CharacterString>
          </cit:processingLevel>
        </cit:CI_OnlineResource>
      </cit:onlineResource>
    </mri:MD_DataIdentification>
  </mdb:identificationInfo>
  <!-- unsure about how to start LI_Lineage, working from example on p62 of ISO19115-3:2016 doc -->
  <mdb:resourceLineage>
    <mrl:LI_Lineage>
      <mrl:statement>
        <gco:CharacterString>history</gco:CharacterString>
      </mrl:statement>
      <mrl:scope>
        <!-- Or should we use https://standards.iso.org/iso/19115/resources/Codelists/gml/MD_ScopeCode.xml -->
        <mcc:MD_ScopeCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#MD_ScopeCode" codeListValue=""></mcc:MD_ScopeCode>
      </mrl:scope>
      <mrl:additionalDocumentation>
        <cit:CI_Citation>
          <cit:title>
            <gco:CharacterString>lineage_statement</gco:CharacterString>
          </cit:title>
          <cit:onlineResource>
            <cit:CI_OnlineResource>
              <cit:linkage>
                <gco:CharacterString></gco:CharacterString>
              </cit:linkage>
            </cit:CI_OnlineResource>
          </cit:onlineResource>
        </cit:CI_Citation>
      </mrl:additionalDocumentation>
      <!-- TODO this is probably repeatable -->
      <mrl:processStep>
        <mrl:LI_ProcessStep>
          <mrl:description>
            <gco:CharacterString>process_step_1_description</gco:CharacterString>
          </mrl:description>
          <mrl:rationale>
            <gco:CharacterString></gco:CharacterString>
          </mrl:rationale>
        </mrl:LI_ProcessStep>
      </mrl:processStep>
      <mrl:source>
        <mrl:LI_Source>
          <mrl:description>
            <!-- TODO what is this for? -->
            <gco:CharacterString>source_description</gco:CharacterString>
          </mrl:description>
          <mrl:sourceSpatialResolution>
            <mri:MD_Resolution>
              <mri:equivalentScale>
                <mri:MD_RepresentativeFraction>
                  <mri:denominator>
                    <gco:Integer></gco:Integer>
                  </mri:denominator>
                </mri:MD_RepresentativeFraction>
              </mri:equivalentScale>
            </mri:MD_Resolution>
          </mrl:sourceSpatialResolution>
          <mrl:sourceReferenceSystem>
            <gex:MD_ReferenceSystem>
              <gex:referenceSystemIdentifier>
                <mcc:MD_Identifier>
                  <mcc:code>
                    <gco:CharacterString>lineage_reference_code</gco:CharacterString>
                  </mcc:code>
                  <mcc:description>
                    <gco:CharacterString>lineage_reference_description</gco:CharacterString>
                  </mcc:description>
                </mcc:MD_Identifier>
              </gex:referenceSystemIdentifier>
              <gex:referenceSystemType>
                <gex:MD_ReferenceSystemTypeCode codeList="https://standards.iso.org/iso/19115/resources/Codelists/cat/codelists.xml#MD_ReferenceSystemTypeCode" codeListValue=""></gex:MD_ReferenceSystemTypeCode>
              </gex:referenceSystemType>
            </gex:MD_ReferenceSystem>
          </mrl:sourceReferenceSystem>
        </mrl:LI_Source>
      </mrl:source>
    </mrl:LI_Lineage>
  </mdb:resourceLineage>
</mdb:MD_Metadata>
